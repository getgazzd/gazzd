{
  "name": "gazzd",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "test": "jest",
    "clear_jest": "jest --clearCache",
    "start:local": "cp .env.local.sample .env && next dev",
